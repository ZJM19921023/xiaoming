function padleft(str,len,cha)
{
    var chalist='';
    for(var i=0;i<len-str.toString().length;i++)
    {
        chalist+=cha;
    }
    
    return chalist+str;
}

function setStuNum(num)
{
    var strNum=padleft(num,7,'0');
    $('#Ls1').html(strNum.substr(0,1));
    $('#Ls2').html(strNum.substr(1,1));
    $('#Ls3').html(strNum.substr(2,1));
    $('#Ls4').html(strNum.substr(3,1));
    $('#Ls5').html(strNum.substr(4,1));
    $('#Ls6').html(strNum.substr(5,1));
    $('#Ls7').html(strNum.substr(6,1));
}

function useBankcode(id,bankcode)
{
	dialogClose();

	title="名校贷镑客码验证";
	$("#bankcodetips").text("镑客码是镑客特权II，只作为奖励给予有特殊贡献的老会员。");
	$("#bankcode").val("请输入MX开头的镑客码");
	$("#bankcode").val(bankcode);
	
	if(id=="limit1")
	{
		title="名校贷应急包镑客码验证";
		$("#bankcodetips").text("应急包镑客码是镑客特权III，只作为奖励给予有特殊贡献的老会员。");
		$("#bankcode").val("请输入YJB开头的镑客码");
		$("#bankcode").val(bankcode);
		
	}else if(id=="limit5"){
	
	    title="白领包镑客码验证";
		$("#bankcodetips").text("白领包镑客码是镑客特权IIII，只作为奖励给予有特殊贡献的老会员。");
		$("#bankcode").val("请输入BLB开头的镑客码");
		$("#bankcode").val(bankcode);
		
	}else if(id=="limit6"){
	    title="专科包镑客码验证";
		$("#bankcodetips").text("专科包镑客码是镑客特权IIII，只作为奖励给予有特殊贡献的老会员。");
		$("#bankcode").val("请输入ZKB开头的镑客码");
		$("#bankcode").val(bankcode);
	}
	$("#dialogDiv").hide();
	tipsWindown(title, "id:showBankCode", "420", "320", "true", "", "true", "book_mark.gif","false");
	
}

function checkBankcode()
