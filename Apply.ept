<script>
function slideData(){
	var slideNo=0;
	var unitWidth=$(".wy-limit-charts ul").find("li").eq(0).outerWidth();
		$(".wy-limit-charts ul").width(function(){
		  return reCountLength()*unitWidth;
		})
		function reCountLength(){
			var ullength=$(".wy-limit-charts ul").find("li").length/2;
				return Math.ceil(ullength);	
		}
		
		 $(".arrow-left").unbind().bind("click",function(e){
             slideNo--;
             if(slideNo<0){slideNo=0; return;}
             slideAnimateTable(-unitWidth*slideNo);
             e.stopPropagation();
        })
        $(".arrow-right").unbind().bind("click",function(e){
             slideNo++;
             if(slideNo>reCountLength()-2){slideNo=reCountLength()-2; return;}
             slideAnimateTable(-unitWidth*slideNo);
             e.stopPropagation();
        })
        function slideAnimateTable(slideNum){
		       
             $(".wy-limit-charts-box ul").stop().animate({"left":slideNum+"px"});
        }

	$(".icon_foot").unbind().bind("click",function(){
		var _this=$(this);
		if($(".big_foot").length>0){ return;}
		var bigfoot=$("<img class='big_foot' height='300' style='position:absolute; left:50%; z-index:1030; top:50%; margin-left:-85px; margin-top:-150px;' src='/Public/images/student/icon_foot_big.png' />");
		$("body").append(bigfoot);
		bigfoot=null;
		var bigfootleft=_this.parents("li").eq(0).offset().left+parseInt(_this.parents("li").eq(0).width()-50);
		var bigfoottop=_this.parents("li").eq(0).offset().top+parseInt(_this.parents("li").eq(0).height()*2-20);
		var bigfootheight=80;
		
		$(".big_foot").stop().animate({"left":bigfootleft+"px","top":bigfoottop+"px","height":bigfootheight+"px"},"fast",function(){
			_this.parents("div.dialogbox").addClass("shake shake-constant");
			setTimeout(function(){_this.parents("div.dialogbox").removeClass("shake shake-constant")},200)
			$(".big_foot").fadeOut("normal",function(){
				$(".big_foot").remove();
				});
			})
		
		})
			
}

</script>
